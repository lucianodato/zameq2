@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#>.
@prefix ue: <http://lv2plug.in/ns/extensions/units#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix param: <http://lv2plug.in/ns/ext/parameters#>.
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#>.

<http://zamaudio.com/lv2/zameq2>
  a lv2:Plugin;
  a lv2:FilterPlugin;
  a lv2:EQPlugin;

  lv2:binary <zameq2.so>;
  doap:name "ZamEQ2";
  doap:maintainer [
     foaf:name "Damien Zammit";
     foaf:homepage <http://zamaudio.com>;
     foaf:mbox <mailto:damien@zamaudio.com>;
  ];
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  lv2:optionalFeature pprops:supportsStrictBounds ;
  lv2:optionalFeature lv2:hardRTCapable ;

  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "output";
    lv2:name "Output";
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "boostdbl";
    lv2:name "Boost/Cut Lowshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:designation param:gain ;
    lv2:minimum -20.0 ;
    lv2:maximum 20.0 ;
    lv2:default 0.0 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "slopedbl";
    lv2:name "Slope Lowshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:minimum 1.0;
    lv2:maximum 1.5;
    lv2:default 1.0;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "freql";
    lv2:name "Frequency Lowshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:designation param:cutoffFrequency;
    lv2:default 400.0 ;
    lv2:minimum 15.0 ;
    lv2:maximum 2000.0 ;
    ue:unit ue:hz;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "boostdb1";
    lv2:name "Boost/Cut 1";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:designation param:gain ;
    lv2:minimum -20.0 ;
    lv2:maximum 20.0 ;
    lv2:default 0.0 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "q1";
    lv2:name "Q 1";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:minimum 0.01 ;
    lv2:maximum 50.0 ;
    lv2:default 0.128 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "freq1";
    lv2:name "Frequency 1";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:designation param:cutoffFrequency;
    lv2:default 250.0 ;
    lv2:minimum 30.0 ;
    lv2:maximum 6000.0 ;
    ue:unit ue:hz;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "boostdb2";
    lv2:name "Boost/Cut 2";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:designation param:gain ;
    lv2:minimum -20.0 ;
    lv2:maximum 20.0 ;
    lv2:default 0.0 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:symbol "q2";
    lv2:name "Q 2";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:minimum 0.01 ;
    lv2:maximum 50.0 ;
    lv2:default 0.264 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 10;
    lv2:symbol "freq2";
    lv2:name "Frequency 2";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:designation param:cutoffFrequency;
    lv2:default 1000.0 ;
    lv2:minimum 500.0 ;
    lv2:maximum 18000.0 ;
    ue:unit ue:hz;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 11;
    lv2:symbol "boostdbh";
    lv2:name "Boost/Cut Highshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:designation param:gain ;
    lv2:minimum -20.0 ;
    lv2:maximum 20.0 ;
    lv2:default 0.0 ;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 12;
    lv2:symbol "slopedbh";
    lv2:name "Slope Highshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:minimum 1.0;
    lv2:maximum 1.5;
    lv2:default 1.0;
    ue:unit ue:coef;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 13;
    lv2:symbol "freqh";
    lv2:name "Frequency Highshelf";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:designation param:cutoffFrequency;
    lv2:default 11000.0 ;
    lv2:minimum 1000.0 ;
    lv2:maximum 20000.0 ;
    ue:unit ue:hz;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 14;
    lv2:symbol "typemlp";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:name "Type Lowpass";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 2.0 ;
    lv2:portProperty lv2:enumeration ;
    lv2:scalePoint [
    		rdfs:label "Bypass" ;
		rdf:value 0.0
		] , [
		rdfs:label "6 db/oct" ;
		rdf:value 1.0
		] , [
		rdfs:label "12 db/oct" ;
		rdf:value 2.0
		]
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 15;
    lv2:symbol "freqmlp";
    lv2:name "Frequency Lowpass";
    lv2:portProperty pprops:hasStrictBounds ;
    lv2:portProperty pprops:logarithmic ;
    lv2:designation param:cutoffFrequency ;
    lv2:default 22000.0 ;
    lv2:minimum 20.0 ;
    lv2:maximum 22000.0 ;
    ue:unit ue:hz ;
  ].



